{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "scopeName": "text.js.swagger-jsdocs-comment",
	"injectionSelector": "L:comment.block.documentation",
  "patterns": [
		{
			"include": "#embedded-yaml"
		}
  ],
  "repository": {
    "embedded-yaml": {
			"name": "meta.embedded.block.yaml",
			"begin": "@openapi\\n(.)*",
			"end": "*/"
    }
  }
}
